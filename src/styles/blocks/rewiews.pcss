.rewiews {
  position: relative;
  padding-bottom: 203px;
  @include tablets {
    padding-bottom: 15.2%;
  }
  @include phones {
    padding-bottom: 45.3%;
  }
}
.rewiews__title {
  font-size: 36px;
  font-weight: 700;
  padding-bottom: 100px;
  @include desktop {
    margin-left: -2px;
  }
  @include tablets {
    font-size: 30px;
    padding-bottom: 70px;
  }
  @include phones {
    font-size: 24px;
    
    padding-bottom: 104px;
  }
}
