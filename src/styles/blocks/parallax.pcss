.parallax {
  position: absolute;
  top: 0;
  left: 0;
  height: 100vh;
  width: 100%;
  
}

.parallax__layer {
  position: absolute;
  top: 0;
  left: 0;
  
  &--1 {

  }

  &--2 {
    top: 31%;
  }

  &--3 {
    top: 46%;
    left: 63%;
  }

  &--4 {
    top: 59%;
  }

  &--5 {
    top: 68%;
    left: -5%;
  }

  &--6 {
    top: 68%;
    left: auto;
    right: 0;
  }
  &--7 {
    top: 130%;
  }
}

.parallax__pic {}